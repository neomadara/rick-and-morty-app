{
  "preset": "react-native",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "transformIgnorePatterns": ["node_modules/(?!axios)/"],
  "testMatch": ["<rootDir>/__tests__/unit/**/*.test.{ts,tsx}"],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  }
}
